        #include <bits/stdc++.h>
        #define ll long long int
        #define nline '\n'
        using namespace std;
         
        int solve()
        {
            string s;
            cin >> s;
         
            if (s == "++X" || s == "X++")   
                return 1;
            else
                return -1;
        }
         
        int main()
        {
            ios_base::sync_with_stdio(false);
            cin.tie(NULL);
            ll T = 1;
            cin >> T;
            ll Ans = 0;
            while (T--) 
            {
                Ans += solve();
            }
            cout << Ans << nline;
        }

